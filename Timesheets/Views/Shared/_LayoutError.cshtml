<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/jqueryajax")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/gridmvc")
    @Styles.Render("~/Content/css")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top" style="background-color:#ffffff;">
        <div class="container">
            <div class="row topheader">
                <div class="pull-right">
                    <div class="print-only">
                        <img src="~/Content/TimeKeeper_Header.png" alt="CushwakeNM" width="1170" height="90" />
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="nav" style="margin-top:25px">
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - TimeKeeper</p>
        </footer>
    </div>

    @RenderSection("scripts", required: false)
</body>
</html>
